{
    "webPort": 8000,

    "mongodbUrl": "mongodb://username:password@localhost/database",

    "gcm": {
        "apiKey": "YOUR_API_KEY_HERE"
    },
    "apn": {
        "connection": {
            "gateway": "gateway.sandbox.push.apple.com",
            "cert": "/path/to/cert.pem",
            "key": "/path/to/key.pem"
        },
        "feedback": {
            "address": "feedback.sandbox.push.apple.com",
            "cert": "/path/to/cert.pem",
            "key": "/path/to/key.pem",
            "interval": 43200,
            "batchFeedback": true
        }
    },
    "removeDuplicatedDevices": true,
    "urlPrefix": "myapp",
    "credentials": [
        {
            "user": "user1",
            "password": "password1",
            "scopes": ["register", "send"]
        },
        {
            "user": "user2",
            "password": "password2",
            "scopes": ["register"]
        }
    ]
}